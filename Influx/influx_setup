#!/usr/bin/expect
spawn influx setup
expect "Please type your primary username:"
send "admin"
send "\r"
expect "Please type your password:"
send "admin123"
send "\r"
expect "Please type your password again:"
send "admin123"
send "\r"
expect "Please type your primary organization name:"
send "iot-org"
send "\r"
expect "Please type your primary bucket name:"
send "iot-demo"
send "\r"
expect "Or press ENTER for infinite.:"
send "168"
send "\r"
expect "Confirm? (y/n):"
send "y"
send "\r" 
expect eof